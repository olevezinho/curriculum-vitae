     @* /********************/
        /****** Header ******/
        /********************/ *@

        header {
            position: relative;
            background-repeat: no-repeat;
            background-size: cover;
            height: 400px;
            margin: 0;
            padding: 0;
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center;
        }

        header::before {
            content: "";
            display: block;
            position: absolute;
            left: 0;
            right: 0;
            bottom: 0;
            top: 0;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: -1;
        }

        header::after {
            content: "";
            display: block;
            position: absolute;
            left: 0;
            right: 0;
            bottom: 0;
            top: 0;
            background-image: url('/curriculum-vitae/header.jpg');
            background-position: center top -450px;
            background-repeat: no-repeat;
            background-size: cover;
            filter: grayscale(100%);
            z-index: -2;
        }

        .circle {
            position: absolute;
            top: 25%;
            left: 15%;
            transform: translateX(-50%);
            width: 200px;
            height: 200px;
            border-radius: 50%;
            overflow: hidden;
            border: 2px solid rgb(172, 172, 172);
            z-index: 1;
        }

        .profile-pic {
            display: block;
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .text-container {
            position: absolute;
            left: 0;
            right: 0;
            top: 50%;
            transform: translateY(-50%);
            color: white;
            text-shadow: 1px 1px 1px black;
        }

        .text-container div::before {
            content: "";
            display: block;
            position: absolute;
            left: 0;
            right: 0;
            bottom: 0;
            top: 0;
            width: 100%;
            background-color: rgba(255, 255, 255, 0.2);
            z-index: -1;
            border-top: 1px solid rgb(138, 136, 136);
            border-bottom: 1px solid rgb(138, 136, 136);
        }

        .text-container h1 .waterblue {
            color: #089288;
        }

        .text-container h1 {
            font-size: 3.5em;
        }

        .text-container p .waterblue {
            color: #089288;
        }

        .text-container p {
            font-size: 2.7em;
            margin: 0;
        }

     @* /********************/
        /** Main container **/
        /********************/ *@

        .main-container {
            display: flex;
            flex-direction: row;
            width: 100%;
            height: 2700px;
        }

        .main-container h1 .black {
            color: #000;
        }

        .main-container p .gray {
            color: #303030;
        }

        .main-container h1 {
            font-size: 2.3em;
        }

        .main-container h2 {
            font-size: 1.3em;
            font-weight: bold;
            color: #707070;
        }

        .main-container h3 {
            font-size: 1.2em;
            color: #575555;
        }

        .main-container p {
            font-size: 1.3em;
        }

        .main-container .date {
            display: inline-block;
            font-weight: normal;
            margin-top: 35px;
            margin-right: 35px;
        }

        .main-container .date span {
            color: #089288;
        }

        .main-container .circle-filled {
            display: inline-block;
            width: 15px;
            height: 15px;
            border-radius: 50%;
            background-color: #033a36;
            border: 1px solid #012523;;
            margin-right: 2px;
            margin-left: 2px;
        }

        .main-container .circle-empty {
            display: inline-block;
            width: 15px;
            height: 15px;
            border-radius: 50%;
            background-color: #b8b8b8;
            border: 1px solid rgb(172, 172, 172);
            margin-right: 2px;
            margin-left: 2px;
        }

        .main-container .circles {
            display: flex;
            width: 100px;
        }

        .left-column,
        .right-column {
            display: flex;
            width: 50%;
            height: 100%;
            box-sizing: border-box;
        }

        .left-column {
            background-color: rgba(211, 211, 211, 0.63);
            border-right: 5px solid #ccc;
            position: relative;
            text-align: justify;
            z-index: -2;
        }

        .right-column {
            background-color: #19a09727;
            position: relative;
            text-align: justify;
            z-index: -2;
        }

        .triangle-top {
            width: 0;
            height: 0;
            border-top: 400px solid transparent;
            border-bottom: 0 solid transparent;
            border-left: 650px solid #fff;
            position: absolute;
            right: 0;
            top: 0;
            transform: rotate(180deg);
            z-index: -1;
        }

        .triangle-bottom {
            width: 0;
            height: 0;
            border-top: 0 solid transparent;
            border-bottom: 400px solid transparent;
            border-left: 650px solid #fff;
            border-right: 0px solid transparent;
            position: absolute;
            right: 0;
            bottom: 0;
            transform: rotate(180deg);
            z-index: -1;
        }

        .text {
            position: absolute;
            padding: 60px;
        }

